\n      people_that_are_ready: []\n    }\n  },\n  methods: {\n    setData(data) {\n      this.people_that_are_ready = data\n      console.log(data)\n    },\n    goToResults(){\n      this.$router.push('/results')\n    },\n    getDoneWaiting() {\n      let URL = \"http://127.0.0.1:8009/results/ready\"\n      fetch(URL)\n      .then((response) => response.json())\n      .then((data) => {\n        console.log(data)\n        if(data.status){\n          console.log(\"Is ready!\")\n          \n        }\n        else{\n          this.setData(data.people)\n          goToResults()\n        }\n      })\n      .catch(function(error) {\n        console.log(error);\n      });\n    } \n  },\n  mounted() {\n    setInterval(() => {\n        this.getDoneWaiting()}, 1000)\n  }\n\n}\n</script>\n\n<style>\n\n</style>"]}]}