s.myChart.updateSeries(this.series);\n    },\n    getData() {\n\n      let URL = \"http://127.0.0.1:8005/data?amount=15\"\n\n      fetch(URL)\n      .then((response) => response.json())\n      .then((data) => {\n        let datapoints = JSON.parse(data)\n        this.setData(datapoints)\n      })\n      .catch(function(error) {\n        console.log(error);\n      });\n    } \n  },\n  mounted() {\n    setInterval(() => {\n        this.getData()}, 3000)\n  }\n}\n</script>\n\n<style scoped>\n\n</style>"],"sourceRoot":"src/components/home"}]}