A,EAAA;AAAA,OAAA,CAAA,CACA,IADA,CACA,UAAA,QAAA;AAAA,eAAA,QAAA,CAAA,IAAA,EAAA;AAAA,OADA,EAEA,IAFA,CAEA,UAAA,IAAA,EAAA;AACA,QAAA,MAAA,CAAA,YAAA;AACA,OAJA,EAKA,KALA,CAKA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OAPA;AAQA,KA5BA;AA6BA,IAAA,OA7BA,mBA6BA,QA7BA,EA6BA;AAAA;;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,aAAA,QAAA,EADA,CAEA;;AACA,UAAA,GAAA,GAAA,IAAA,GAAA,CAAA,iDAAA,CAAA,CAHA,CAIA;;AACA,MAAA,GAAA,CAAA,YAAA,CAAA,GAAA,CAAA,UAAA,EAAA,QAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AAEA,MAAA,KAAA,CAAA,GAAA,EAAA;AAAA,QAAA,MAAA,EAAA;AAAA,OAAA,CAAA,CACA,IADA,CACA,UAAA,QAAA;AAAA,eAAA,QAAA,CAAA,IAAA,EAAA;AAAA,OADA,EAEA,IAFA,CAEA,UAAA,IAAA,EAAA;AACA,QAAA,MAAA,CAAA,YAAA;AACA,OAJA,EAKA,KALA,CAKA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OAPA;AAQA,KA7CA;AA8CA,IAAA,UA9CA,wBA8CA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,YAAA;AACA,WAAA,OAAA,CAAA,KAAA,YAAA;AACA,WAAA,YAAA,GAAA,EAAA;AACA;AAlDA,GAhBA;AAoEA,EAAA,OApEA,qBAoEA;AACA,SAAA,YAAA;AACA;AAtEA,CAAA","sourcesContent":["<template>\n    <v-container>\n        <v-row>\n            <v-form>\n                <v-text-field label=\"Question\" v-model=\"questionText\">\n                </v-text-field>\n                <v-btn color=\"success\" @click=\"submitItem\">Add question</v-btn>\n            </v-form>\n        </v-row>\n        <v-row>\n    <v-simple-table>\n      <thead>\n          <tr>\n              <th class=\"text-left\">\n                  Question\n              </th>\n              <th>\n                  Delete\n              </th>\n          </tr>\n      </thead>\n      <tbody>\n          <tr v-for=\"question in questionsArray\" :key=\"question.id\">\n              <td> {{question.question}} </td>\n              <v-icon @click=\"deleteItem(question.id)\">fa-trash</v-icon>\n          </tr>\n      </tbody>\n  </v-simple-table>\n        </v-row>\n  \n  </v-container>\n</template>\n\n<script>\nexport default {\n    data() {\n        return {\n            questionsArray: [\n        {\n            \"id\": \"fdde0250-2251-474e-831e-f974f328b6f8\",\n            \"question\": \"Placeholder question?\"\n        },\n        {\n            \"id\": \"53f10a02-7742-4ee2-8fe4-1784248454df\",\n            \"question\": \"Another placeholder question?\"\n        }\n        ],\n        questionText: \"\"\n        }\n    },\n    methods: {\n        getQuestions() {\n            let URL = \"\"\n            URL = \"https://topkerst.icheered.nl/api/admin/question\"\n            //URL = \"http://localhost:8009/admin/question\"\n            fetch(URL)\n            .then((response) => response.json())\n            .then((data) => {\n                this.questionsArray = data\n            })\n            .catch(function(error) {\n                console.log(error);\n            });\n        },\n        deleteItem(id){\n            console.log(id)\n\n            let URL = \"\"\n            URL = \"https://topkerst.icheered.nl/api/admin/question?questionID=\"+id\n            //URL = \"http://localhost:8009/admin/question?questionID=\"+id\n            fetch(URL, {method: 'DELETE'})\n            .then((response) => response.json())\n            .then((data) => {\n                this.getQuestions()\n            })\n            .catch(function(error) {\n                console.log(error);\n            });\n        },\n        addItem(question) {\n            console.log(\"Adding: \" + question)\n            //\"http://localhost:8009/admin/question?question=Is%20this%20a%20test%20question%3F\"\n            var url = new URL(\"https://topkerst.icheered.nl/api/admin/question\")\n            //var url = new URL(\"http://localhost:8009/admin/question\")\n            url.searchParams.set('question', question)\n            console.log(url)\n            \n            fetch(url, {method: 'POST'})\n            .then((response) => response.json())\n            .then((data) => {\n                this.getQuestions()\n            })\n            .catch(function(error) {\n                console.log(error);\n            });\n        },\n        submitItem(){\n            console.log(this.questionText)\n            this.addItem(this.questionText)\n            this.questionText = \"\"\n        }\n    },\n    mounted() {\n        this.getQuestions()\n    },\n}\n</script>\n\n<style>\n\n</style>"],"sourceRoot":"src/views"}]}